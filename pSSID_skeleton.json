{
    "_meta":{
        "display-name": "Example Measurements"
    },

    "archives": {
        "example_rabbitmq_archive": {
            "archiver": "rabbitmq",
            "data": {
                "measurement-agent": "{% scheduled_by_address %}",
                "routing-key": "pscheduler_raw",
                "url": "amqp://elastic:elastic@elastic"
            }
        }
    },

    "addresses": {
        "FILL_IN_ADDRESS_NAME_1": { "address": "FILL_IN_HOSTNAME_OR_IP_1" },
        "FILL_IN_ADDRESS_NAME_2": { "address": "FILL_IN_HOSTNAME_OR_IP_2" }
    },

    "Channel_list": {
	"EduRoam_wlan0_channels": [ 11, 12, 13, 14, 15 ],
	"MWireless_wlan0_channels": [ 1, 2, 3, 4, 5, 10 ]
    }
    
    "SSIDs" : {
	"MWireless_wlan0": {
	    "SSID_name": "MWireless",
	    "BSSID_Channels": "MWireless_wlan0_channels",
	    "BSSID_Channel_mismatch_warning": true,
	    "BSSID_Channel_mismatch_connect": false,
            "BSSID_min_signal": -55,
	    "BSID_min_qualifying": 1,
	    "AuthMethod": {
		"type": "User",
		"UID": "fakeUID",
		"Password": "fakepassword"
	    }
	},
	"EduRoam_wlan0": {
	    "SSID_name": "EduRoam",
	    "BSSID_Channels": "EduRoam_wlan0_channels",
	    "BSSID_Channel_mismatch_warning": true,
	    "BSSID_Channel_mismatch_connect": false,
            "BSSID_min_signal": -55,
	    "BSID_min_qualifying": 0,
	    "AuthMethod": {
		"type": "User",
		"UID": "fakeUID",
		"Password": "fakepassword"
	    }
	}
    },
    
    "SSID_lists": {
	"wlan0_SSID_list": {
	    "SSIDs": [ "MWireless_wlan0", "EduRoam_wlan0" ]
	}
    },

    "SSID_scan": {
	"wlan0_scan": {
	    "network_interface": "wlan0",
	    "SSID_list": "wlan0_SSID_list",
	    "SSID_warn_if_unknown": true,
	    "schedule": [ "schedule_every_5_min" ]
	}
    },

    "groups": {
        "example_group_mesh": {
            "type": "mesh",
            "addresses": [
                { "name": "FILL_IN_ADDRESS_NAME_1" },
                { "name": "FILL_IN_ADDRESS_NAME_2" }
            ]
        },
        
        "example_group_disjoint": {
            "type": "disjoint",
            "a-addresses": [
                { "name": "FILL_IN_ADDRESS_NAME_1" }
            ],
            "b-addresses": [
                { "name": "FILL_IN_ADDRESS_NAME_2" }
            ]
        }
    },
    
    "tests": {
        "example_test_rtt": {
            "type": "rtt",
            "spec": {
                "dest": "testpoint2"
            }
        },
        "example_test_latencybg": {
            "type": "latencybg",
            "spec": {
                "source": "{% address[0] %}",
                "dest": "{% address[1] %}",
                "flip": "{% flip %}"
            }
        },
        "example_test_throughput": {
            "type": "throughput",
            "spec": {
                "dest": "perfsonar-bin-arbl.umnet.umich.edu"
            }
        },
        "example_test_trace": {
            "type": "trace",
            "spec": {
                "source": "{% address[0] %}",
                "dest": "{% address[1] %}"
            }
        }
    },
    
    "schedules": {
        "schedule_every_4_hours": {
            "repeat": "0 */4 * * *"
        },
        "schedule_every_minute": {
            "repeat": "*/1 * * * *"
        },   
        "schedule_every_hour": {
            "repeat": "0 * * * *"
        },   
        "schedule_Tuesday_10AM": {
            "repeat": "0 10 * * 2"
        },   
        "schedule_Tuesday_10:10AM": {
            "repeat": "10 10 * * 2"
        },   
        "schedule_10_minutes": {
            "repeat": "*/10 * * * *"
        }
    },

    "tasks": {
        "example_task_rtt": {
	    "task_SSID_scan": "wlan0_scan",
            "task_SSIDs": [ "MWireless", "EduRoam" ],
            "task_priority": 2,
            "task_test_spec": "example_test_rtt",
            "task_archives": [ "example_rabbitmq_archive" ],
            "task_schedule": [ "schedule_every_hour", "schedule_Tuesday_10:10AM" ],
            "_meta": {
                "display-name": "Example RTT Tests"
            }
        },

        "example_task_throughput": {
	    "task_SSID_scan": "wlan0_scan",
            "task_SSIDs": [ "MWireless", "EduRoam" ],
            "task_priority": 1,
            "task_test_spec": "example_test_throughput",
            "task_archives": [ "example_rabbitmq_archive" ],
            "task_schedule": [ "schedule_every_hour", "schedule_Tuesday_10:10AM" ],
            "_meta": {
                "display-name": "Example RTT Tests"
            }
        }
    }
}
